{
  "expo": {
    "name": "Kataraa",
    "slug": "kataraa-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_final.png",
    "scheme": "kataraa",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash_final.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kataraa.app",
      "infoPlist": {
        "NSSpeechRecognitionUsageDescription": "السماح للتطبيق بالتعرف على الكلام",
        "NSMicrophoneUsageDescription": "السماح للتطبيق باستخدام الميكروفون للبحث الصوتي"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_final.png",
        "backgroundColor": "#667eea"
      },
      "package": "com.kataraa.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "@jamsch/expo-speech-recognition",
        {
          "speechRecognitionPermission": "نحتاج إلى إذن للتعرف على الكلام لتمكين البحث الصوتي.",
          "microphonePermission": "نحتاج إلى إذن للوصول إلى الميكروفون لتمكين البحث الصوتي."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon_final.png",
          "color": "#667eea"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c95b01ab-9fc0-45ed-bf61-aa6b8948c6df"
      }
    }
  }
}